local UILib = loadstring(game:HttpGet('https://raw.githubusercontent.com/SakuraWare/SakuraWare-Ui/refs/heads/main/Sakura%20WareUi2'))()
local Window = UILib.new("Sakura Ware", game.Players.LocalPlayer.UserId, "Member")

-- Kategorie: Farming
local Category1 = Window:Category("Farming", "http://www.roblox.com/asset/?id=8395621517")
local SubButton1 = Category1:Button("Auto Farm", "http://www.roblox.com/asset/?id=8395747586")
local SubButton2 = Category1:Button("Auto Hatch", "http://www.roblox.com/asset/?id=8395747586")

-- Abschnitt: Auto Train
local SectionTrain = SubButton1:Section("Auto Train", "Left")
SectionTrain:Toggle({
    Title = "Auto Train",
    Description = "Fast Muscle gain",
    Default = false
}, function(value)
    _G.AutoTrain = value

    while _G.AutoTrain do
        local args = {
            [1] = true
        }

        game:GetService("ReplicatedStorage").Remote.Event.Train:FindFirstChild("[C-S]PlayerTryClick"):FireServer(unpack(args))
        wait(0.1)
    end
end)

-- Abschnitt: Auto Eat
local SectionEat = SubButton1:Section("Auto Eat", "Left")
SectionEat:Toggle({
    Title = "Auto Eat",
    Description = "Fast Energy gain",
    Default = false
}, function(value)
    _G.AutoEat = value

    while _G.AutoEat do
        game:GetService("ReplicatedStorage").Remote.Event.Food:FindFirstChild("[C-S]TryEatFood"):FireServer()
        wait(0.1)
    end
end)

-- Abschnitt: Auto Money
local SectionMoney = SubButton1:Section("Auto Money", "Left")
SectionMoney:Toggle({
    Title = "Auto Money",
    Description = "Fast Money gain",
    Default = false
}, function(value)
    _G.AutoFight = value

    while _G.AutoFight do
        for i = 1, 30 do
            local args = { "Fight" .. i }
            game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerEnd"):FireServer(unpack(args))
        end
        wait(0.1)
    end
end)

local SectionHatch = SubButton2:Section("Auto Hatch", "Left")
local HatchEggName = "Egg1"
local HatchAmount = 1
_G.AutoHatch = false

-- Dropdown for selecting egg
SectionHatch:Dropdown({
    Title = "Select Egg",
    Description = "Choose between Egg1 and Egg15",
    Values = {
        "Egg1", "Egg2", "Egg3", "Egg4", "Egg5",
        "Egg6", "Egg7", "Egg8", "Egg9", "Egg10",
        "Egg11", "Egg12", "Egg13", "Egg14", "Egg15"
    },
    Default = "Egg1"
}, function(selected)
    HatchEggName = selected
    print("✅ Selected Egg:", HatchEggName)
end)

-- Textbox for hatch amount
SectionHatch:Textbox({
    Title = "Hatch Amount",
    Description = "Number from 1 to 25",
    Default = "1",
}, function(value)
    local num = tonumber(value)
    if num and num >= 1 and num <= 25 then
        HatchAmount = num
        print("✅ Hatch amount set to:", HatchAmount)
    else
        warn("❌ Invalid input! Please enter a number from 1 to 25.")
    end
end)

-- Toggle for auto hatch
SectionHatch:Toggle({
    Title = "Auto Hatch",
    Description = "Automatically hatch eggs",
    Default = false
}, function(toggle)
    _G.AutoHatch = toggle

    if toggle then
        spawn(function()
            while _G.AutoHatch do
                local success, err = pcall(function()
                    local args = {
                        HatchEggName,
                        HatchAmount
                    }

                    game:GetService("ReplicatedStorage")
                        :WaitForChild("Remote")
                        :WaitForChild("Function")
                        :WaitForChild("Luck")
                        :WaitForChild("[C-S]DoLuck"):InvokeServer(unpack(args))
                end)

                if not success then
                    warn("❌ Hatch failed:", err)
                end

                wait(1)
            end
        end)
    end
end)





-- Kategorie: Dupe Rewards
local Category2 = Window:Category("Dupe Rewards", "http://www.roblox.com/asset/?id=8395621517")

local SubButtonDupe1 = Category2:Button("Dupe Forever Pack", "http://www.roblox.com/asset/?id=8395747586")
local SubButtonDupe2 = Category2:Button("Dupe Event Pass", "http://www.roblox.com/asset/?id=8395747586")
local SubButtonDupe3 = Category2:Button("Dupe Season Pass", "http://www.roblox.com/asset/?id=8395747586")


local Section1 = SubButtonDupe1:Section("Forever Pack Dupe", "Left")
local DupeID = 1
local _G = _G or {}
_G.DupeItems = false

Section1:Toggle({
    Title = "Dupe Items",
    Description = "Dupe Forever Pack",
    Default = false
}, function(toggle)
    _G.DupeItems = toggle

    if toggle then
        spawn(function()
            while _G.DupeItems do
                local success, err = pcall(function()
                    local remote = game:GetService("ReplicatedStorage")
                        :WaitForChild("Remote")
                        :WaitForChild("Event")
                        :WaitForChild("ForverPack") -- ← überprüfe: eventuell Tippfehler
                        :WaitForChild("[C-S]TryClaimForverPack")

                    remote:FireServer(DupeID)
                end)

                if not success then
                    warn("❌ Error:", err)
                end

                wait(0.1)
            end
        end)
    end
end)

Section1:Textbox({
    Title = "Item ID",
    Description = "Number from 1 to 200",
    Default = "1",
}, function(value)
    local num = tonumber(value)
    if num and num >= 1 and num <= 200 then
        DupeID = num
        print("✅ Item ID set to:", DupeID)
    else
        warn("❌ Invalid input! Please enter a number from 1 to 200.")
    end
end)

local Section2 = SubButtonDupe2:Section("Event Reward Dupe", "Left")
local EventRewardID = 1
local _G = _G or {}
_G.DupeEventRewards = false

Section2:Toggle({
    Title = "Dupe Rewards",
    Description = "Dupe Event Pass",
    Default = false
}, function(toggle)
    _G.DupeEventRewards = toggle

    if toggle then
        spawn(function()
            while _G.DupeEventRewards do
                local success, err = pcall(function()
                    local remote = game:GetService("ReplicatedStorage")
                        :WaitForChild("Remote")
                        :WaitForChild("Event")
                        :WaitForChild("EventSeason")
                        :WaitForChild("[C-S]TryGetReward")

                    local args = {
                        [1] = "1",               -- feste ID
                        [2] = EventRewardID      -- variable ID
                    }

                    remote:FireServer(unpack(args))
                end)

                if not success then
                    warn("❌ Error:", err)
                end

                wait(0.1)
            end
        end)
    end
end)

Section2:Textbox({
    Title = "Reward ID",
    Description = "Number from 1 to 15",
    Default = "1",
}, function(value)
    local num = tonumber(value)
    if num and num >= 1 and num <= 15 then
        EventRewardID = num
        print("✅ Event Reward ID set to:", EventRewardID)
    else
        warn("❌ Invalid input! Please enter a number from 1 to 15.")
    end
end)

