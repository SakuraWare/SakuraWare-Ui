local UILib = loadstring(game:HttpGet('https://raw.githubusercontent.com/SakuraWare/SakuraWare-Ui/refs/heads/main/Sakura%20WareUi2'))()
local Window = UILib.new("Sakura Ware", game.Players.LocalPlayer.UserId, "Member")

local Category1 = Window:Category("Farming", "http://www.roblox.com/asset/?id=8395621517")
local SubButton1 = Category1:Button("Auto Farm", "http://www.roblox.com/asset/?id=8395747586")

local Section1 = SubButton1:Section("Auto Train", "Left")
Section1:Toggle({
    Title = "Auto Train",
    Description = "Fast Muscle gain",
    Default = false
}, function(value)
    _G.AutoFarm = value
    
    while _G.AutoFarm do
        local args = {
            [1] = true
        }

        game:GetService("ReplicatedStorage").Remote.Event.Train:FindFirstChild("[C-S]PlayerTryClick"):FireServer(unpack(args))
        
        wait(0.1) -- Wartezeit, um Spam zu vermeiden
    end
end)

local Section1 = SubButton1:Section("Auto Eat", "Left")
Section1:Toggle({
    Title = "Auto Eat",
    Description = "Fast Energy gain",
    Default = false
}, function(value)
    _G.AutoFarm = value
    
    while _G.AutoFarm do
        game:GetService("ReplicatedStorage").Remote.Event.Food:FindFirstChild("[C-S]TryEatFood"):FireServer()
        
        wait(0.1) -- Wartezeit, um Spam zu vermeiden
    end
end)

local Section1 = SubButton1:Section("Auto Money", "Left")
Section1:Toggle({
    Title = "Auto Money",
    Description = "Fast Money gain",
    Default = false
}, function(value)
    _G.AutoFight = value

    while _G.AutoFight do
        for i = 1, 30 do
            local args = { "Fight" .. i }
            game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerEnd"):FireServer(unpack(args))
        end
        
        wait(0.1) -- Kleine Verz√∂gerung zur Optimierung
    end
end)

local Category2 = Window:Category("Dupe Rewards", "http://www.roblox.com/asset/?id=8395621517")

Section1:Slider({
    Title = "Walkspeed",
    Description = "",
    Default = 16,
    Min = 0,
    Max = 120
    }, function(value)
    print(value)
end)
Section1:ColorPicker({
    Title = "Colorpicker",
    Description = "",
    Default = Color3.new(255,0,0),
    }, function(value)
    print(value)
end)
Section1:Textbox({
    Title = "Damage Multiplier",
    Description = "",
    Default = "",
    }, function(value)
    print(value)
end)
Section1:Keybind({
    Title = "Kill All",
    Description = "",
    Default = Enum.KeyCode.Q,
    }, function(value)
    print(value)
end)
